{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${description}",
  "authors": [
    "${mod_author}"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },
  "license": "${license}",
  "icon": "icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.restonic4.fancyweather.Fancyweather"
    ],"fabric-gametest": [
      "com.restonic4.fancyweather.gametests.FancyGameTests"
    ]
  },
  "mixins": [
    "${mod_id}.mixins.json",
    "${mod_id}.fabric.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "fabric-api": "*",
    "minecraft": "${minecraft_version}",
    "java": ">=${java_version}",
    "midnightlib": "*"
  },
  "suggests": {
  },
  "custom": {
    "lithium:options": {
      "mixin.world.block_entity_ticking.sleeping.campfire": false,
      "mixin.world.block_entity_ticking.sleeping.campfire.lit": false,
      "mixin.world.block_entity_ticking.sleeping.campfire.unlit": false,
      "mixin.alloc.chunk_random": false
    }
  }
}
